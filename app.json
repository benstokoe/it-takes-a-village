{
  "expo": {
    "name": "it-takes-a-village",
    "slug": "it-takes-a-village",
    "version": "1.0.0",
    "scheme": "it-takes-a-village",
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/playfair-display/PlayfairDisplay_400Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.benstokoe.ittakesavillage",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.benstokoe.ittakesavillage"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e618f34-55ef-4df7-83ee-73a61bc0aa37"
      }
    }
  }
}
